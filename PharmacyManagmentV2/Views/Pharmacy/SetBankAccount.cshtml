@model  PharmacyManagmentV2.Data.BankAccount

@{
    ViewData["Title"] = "Set Bank Account";
    Layout = "Layout";
}

<h1>Set Bank Account</h1>


<form asp-action="SetBankAccount">
    <div class="form-group">
        <label asp-for="Id" class="control-label">Select Bank Account</label>
        <select asp-for="Id" class="form-control" asp-items="ViewBag.BankAccounts"></select>
    </div>
    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-primary" />
    </div>
</form>